{
   "background": {
      "persistent": true,
      "scripts": [ "libs/jquery-3.4.1.min.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/128.png",
      "default_popup": "popup.html",
      "default_title": "Tiga - Github国内下载和克隆文件加速"
   },
   "content_scripts": [ {
      "css": [ "content.css" ],
      "js": [ "libs/jquery-3.4.1.min.js", "content.js" ],
      "matches": [ "*://github.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "解决国内下载和克隆GitHub文件太慢的问题。用上Tiga给GitHub加速，体验下载和克隆飞一般的速度（大概）。",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Tiga - Github国内下载和克隆文件加速",
   "options_ui": {
      "page": "options.html"
   },
   "permissions": [  ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0",
   "web_accessible_resources": [ "libs/*", "images/*" ]
}
